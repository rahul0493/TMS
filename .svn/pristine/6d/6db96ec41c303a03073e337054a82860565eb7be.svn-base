/*-----------tab---------*/
 .tab {
     border:1px solid #31708e;
     border-radius:3px;
     overflow: hidden;
     background-color: #f1f1f1;
}
/* Style the buttons inside the tab */
 .tab button {
     background-color: inherit;
     float: left;
     border: none;
     outline: none;
     cursor: pointer;
     padding: 10px;
     transition: 0.3s;
     font-size: 17px;
}
/* Change background color of buttons on hover */
 .tab button {
     border-radius:1px;
     border-right:0.5px solid #31708e ;
     background-color: #1a3a4a38;
     color: #1a3a4a;
}
 .tab button:hover,.tab button.active:hover {
    /* font-size:18px;
     */
     border-right:1px solid #31708e;
     background-color: #f5f5f5 !important;
     color: #1a3a4a;
}
/* Create an active/current tablink class */
 .tab button.active {
     background-color: #31708e !important;
     color: white;
}
/* Style the tab content */
 .tabcontent {
 background: #eaeaea87;
     margin-bottom:15px;
     display: none;
     padding: 6px 12px;
     border: 1px solid #ccc;
     border-top: none;
}
