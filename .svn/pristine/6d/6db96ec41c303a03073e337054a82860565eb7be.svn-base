.toggleSidenav {
	position: relative;
    padding: 5px 5px;
    margin-bottom: 5px;
    margin-top: 5px;
    margin-left: 5px;
	background-color: transparent;
	background-image: none;
	border: 1px solid transparent;
	border-radius: 4px
}

.toggleSidenav:focus {
	outline: 0
}

.toggleSidenav:hover {
 background-color: #6f8088;
}
.toggleSidenav .icon-bar {
	display: block;
	width: 22px;
	height: 2px;
	border-radius: 1px
}

.toggleSidenav .icon-bar+.icon-bar {
	margin-top: 4px
}

.toggleSidenav .icon-bar {
	background-color: white;
}
 @media (max-width:768px) {
	.toggleSidenav {
		display: none
	}
} 

